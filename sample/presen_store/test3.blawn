class Animal(name)
    @name = name

class Person(name,pet)
    @name = name
    @pet = pet
    @function hello()
        text = "Hello! I'm "
        text.append(self.name)
        print(text)
        return

class Car(driver)
    @driver = driver

function change_driver(car)
    pet = Animal("dog")
    bob = Person("Bob",pet)
    new_car = Car(bob)
    car <- new_car
    return

cat = Animal("cat")
john = Person("john",cat)
car = Car(john)
print(car.driver.pet.name)

change_driver(car)

print(car.driver.pet.name)